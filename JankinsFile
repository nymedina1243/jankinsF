pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat 'echo "hola mundo"'
                bat 'dir'
            }
        }
        stage('Promote Artifact') {
            steps {
                bat 'echo "hola mundo"'
                bat 'mkdir hola mundo'
                bat 'cd hola mundo'
                bat 'echo aqui estoy adentro de hola mundo'
            }
        }
    }
}